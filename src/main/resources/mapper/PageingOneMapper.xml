<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.PagingDao">

    <select id="findByPage" resultType="com.example.demo.entity.po.Material" parameterType="map">
        select * from t_zjds_oa_material limit #{page},#{size}
    </select>


    <select id="getCount" resultType="Long">
	select count(1) from t_zjds_oa_material
    </select>

    <select id="getDataList" resultType="com.example.demo.entity.po.Material">
        select * from t_zjds_oa_material
    </select>

</mapper>
